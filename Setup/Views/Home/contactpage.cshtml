<link rel="stylesheet" href="~/StyleSheets/contactpageStyle.css">

        <body>

            <div class="page">
                <section class="contact">

                    <h1>Contact</h1>

                    <form action="" method="post" class="form-contactpagina">

                        <div class="form-contactpagina__inputelement fx-col">
                    <label for="title">Title: </label>
                    <input type="text" name="title" id="title" maxlength="200" required>
                        </div>

                        <div class="form-contactpagina__inputelement fx-col">
                            <label for="email">Email: </label>
                    <input type="email" name="email" id="email" required>
                        </div>

                        <div class="form-contactpagina__inputelement fx-col">
                            <label for="message">Message: </label>
                    <input type="text" name="message" id="message" maxlength="600" required>
                        </div>

                       

                        <div class="form-contactpagina__inputelement">
                            <input type="submit" value="send">
                        </div>

               

                        /*student uitwerking*/


                    </form>


                </section>
            </div>

        </body>
